container_commands:
  01-change_permmision:
    command: "sudo chown -R www-data:www-data /var/www/html"
  02-copy_env:
    command: "sudo cp /var/app/.env /var/www/html/.env"
  03-change_inner_permission:
    command: "sudo chown -R www-data:www-data /var/www/html/*"
  04-change_storage_permission:
    command: "sudo chgrp -R www-data /var/www/html/storage"
  05-change_storage_permission:
    command: "sudo chmod -R o+w /var/www/html/storage"